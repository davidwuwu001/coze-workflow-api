<svg width="192" height="192" viewBox="0 0 192 192" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- 背景圆形 -->
  <circle cx="96" cy="96" r="96" fill="#3b82f6"/>
  
  <!-- 渐变定义 -->
  <defs>
    <linearGradient id="gradient1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#60a5fa;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1d4ed8;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="gradient2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#e5e7eb;stop-opacity:0.8" />
    </linearGradient>
  </defs>
  
  <!-- 主要图标 - 代表工作流的连接节点 -->
  <g transform="translate(96,96)">
    <!-- 中心节点 -->
    <circle cx="0" cy="0" r="15" fill="url(#gradient2)" stroke="#1e40af" stroke-width="2"/>
    
    <!-- 上方节点 -->
    <circle cx="0" cy="-30" r="9" fill="url(#gradient2)" stroke="#1e40af" stroke-width="1.5"/>
    <line x1="0" y1="-15" x2="0" y2="-21" stroke="#ffffff" stroke-width="2" stroke-linecap="round"/>
    
    <!-- 左方节点 -->
    <circle cx="-30" cy="0" r="9" fill="url(#gradient2)" stroke="#1e40af" stroke-width="1.5"/>
    <line x1="-15" y1="0" x2="-21" y2="0" stroke="#ffffff" stroke-width="2" stroke-linecap="round"/>
    
    <!-- 右方节点 -->
    <circle cx="30" cy="0" r="9" fill="url(#gradient2)" stroke="#1e40af" stroke-width="1.5"/>
    <line x1="15" y1="0" x2="21" y2="0" stroke="#ffffff" stroke-width="2" stroke-linecap="round"/>
    
    <!-- 下方节点 -->
    <circle cx="0" cy="30" r="9" fill="url(#gradient2)" stroke="#1e40af" stroke-width="1.5"/>
    <line x1="0" y1="15" x2="0" y2="21" stroke="#ffffff" stroke-width="2" stroke-linecap="round"/>
    
    <!-- 中心图标 - C字母代表Coze -->
    <text x="0" y="5" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#1e40af">C</text>
  </g>
  
  <!-- 装饰性元素 -->
  <circle cx="48" cy="48" r="3" fill="#ffffff" opacity="0.6"/>
  <circle cx="144" cy="48" r="2" fill="#ffffff" opacity="0.4"/>
  <circle cx="48" cy="144" r="2" fill="#ffffff" opacity="0.4"/>
  <circle cx="144" cy="144" r="3" fill="#ffffff" opacity="0.6"/>
</svg>